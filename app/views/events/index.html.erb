<h1>Meetup Search</h1>
<br>
<%= render partial: 'form' %>
<br>

<div id='loading-message'></div>

<table>
    <thead>
      <tr>
        <th>Image</th>
        <th>Name</th>
        <th>Location</th>
        <th>Status</th>
        <th>Category</th>
      </tr>
    </thead>
    <tbody id='events-list'>
      <%= render partial: 'event', collection: @events, as: :event %>
    </tbody>
</table>
